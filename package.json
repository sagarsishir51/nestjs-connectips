{
  "name": "nestjs-connectips",
  "version": "0.0.4",
  "description": "This is simple wrapper for ConnectIPS Payment  @nestjs-connectips. Just ping us or open pull request and contribute :)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "jest --detectOpenHandles",
    "prepublish": "npm run build"
  },
  "keywords": [
    "nestjs",
    "connectips",
    "api"
  ],
  "author": "Sagar Bhusal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sagarsishir51/nestjs-connectips"
  },
  "dependencies": {
    "@nestjs/axios": "^3.1.3",
    "crypto-js": "^4.2.0",
    "node-forge": "^1.3.1",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.30",
    "jest": "^29.7.0",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.3.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.20"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
